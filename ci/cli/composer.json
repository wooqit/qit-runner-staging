{
    "name": "qit/ci-cli",
    "description": "A CLI tool to manage QIT GitHub Workflows.",
    "type": "cli-app",
    "require": {
        "php": "^7.2.5 | ^8",
        "ext-curl": "*",
        "ext-zip": "*",
        "symfony/console": "^5.4",
        "lucatume/di52": "^3.3",
        "symfony/process": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "spatie/phpunit-snapshot-assertions": "^4"
    },
    "autoload": {
        "psr-4": {
            "CI_CLI\\": "src/"
        }
    },
    "config": {
        "platform": {
            "php": "7.4.33"
        }
    }
}
